{
  "name": "ytdl",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "description": "help you download them thicc youtube vids",
  "author": "MelonPanGames",
  "main": "public/electron.js",
  "build": {
    "appId": "com.melonpangames.youtube-downloader",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "asarUnpack": [
      "**/app/node_modules/ffmpeg/*",
      "**/app/node_modules/youtube-dl/*"
    ],
    "win": {
      "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.5.0",
    "electron-is-dev": "^1.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "ffmpeg": "0.0.4",
    "formik": "^2.1.4",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "youtube-dl": "^3.0.2",
    "yup": "^0.28.5"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "clean": "rimraf build && rimraf dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run clean && npm run build",
    "react-start": "BROWSER=none react-scripts start",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^22.6.0",
    "rimraf": "^3.0.2"
  }
}
